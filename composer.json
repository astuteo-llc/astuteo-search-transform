{
    "name": "astuteo/astuteo-search-transform",
    "description": "Craft 3 helper to modify text on projects Astuteo is using Algolia search on.",
    "type": "craft-plugin",
    "version": "2.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Astuteo",
            "homepage": "https://astuteo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
          "astuteo\\astuteosearchtransform\\": "src/"
        }
    },

    "extra": {
        "name": "Astuteo Search Transform",
        "handle": "astuteo-search-transform",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/astuteollc/astuteo-search-transform/master/CHANGELOG.md",
        "components": {
            "astuteoSearchTransformService": "astuteo\\astuteosearchtransform\\services\\AstuteoSearchTransformService"
        },
        "class": "astuteo\\astuteosearchtransform\\AstuteoSearchTransform"
    }
}
